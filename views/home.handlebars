{{> navbar}}

<div class="appNav">
	<div class="appNavInner">
		<div class="wikiwebLogo">
			<a href="/">
				<span>WikiWeb</span>
			</a>
		</div>
		<form id="askform" class="searchBox" action="/search" method="get">
			<input
				type="text"
				id="email"
				name="q"
				placeholder="Ask or Search the WikiWeb"
				onkeypress="searchOnEnter()"
			>{{inputURI}}</input>
			<input id="navbarSearchButton" class="submitButton" type="submit" value="Submit"></input>
		</form>
		<div class="pageButtons">
			<div class="pageButton">
				<div class="buttonContents currentPage">
					<span class="icon">
						<i class="fa fa-hand-paper-o" aria-hidden="true"></i>
					</span>
					<span class="text">Requests</span>
				</div>
			</div>
			<div class="pageButton">
				<div class="buttonContents">
					<span class="icon">
						<i class="fa fa-bullseye" aria-hidden="true"></i>
					</span>
					<span class="text">Connections</span>
				</div>
			</div>
			<div class="pageButton">
				<div class="buttonContents">
					<span class="icon">
						<i class="fa fa-bell-o" aria-hidden="true"></i>
					</span>
					<span class="text">Notifications</span>
				</div>
			</div>
			<div class="pageButton">
				<div class="buttonContents">
					<span class="userImg">
						<img src="/static/img/profile.jpg" class="userImg" />
					</span>
					<span class="text">User</span>
				</div>
			</div>
		</div>
	</div>
</div>
{{> messages}}

<div class="homePage">
	{{> modal}}
	<div class="contentWrapper">
		<div class="layout_3col_left">
			<h3 class="title">
				<div class="title_with_link">
					Feeds
